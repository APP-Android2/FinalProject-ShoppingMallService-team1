<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <!-- 테두리 색상과 두께 설정 -->
    <stroke
        android:width="1dp"
        android:color="@color/Pup_Color" />

    <!-- 버튼의 배경 색상을 여기에 지정 -->
    <solid
        android:color="@color/white" />

    <!-- 버튼의 모서리 설정 -->
    <corners
        android:radius="10dp" />
</shape>