<?xml version="1.0" encoding="utf-8"?>

<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="@dimen/Side_Padding"
        tools:context=".fragment.OnBoarding2Fragment">

        <ImageView
            android:id="@+id/main_imageView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:adjustViewBounds="true"
            android:src="@mipmap/fitmoa_logo_foreground" />

        <TextView
            android:id="@+id/main_Title_textView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="제목222"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="50sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/main_Detail_textView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/Up_Down_Margin"
            android:paddingStart="@dimen/Side_Padding"
            android:paddingEnd="@dimen/Side_Padding"
            android:text="설명222"
            android:textSize="25sp" />

    </LinearLayout>

</layout>